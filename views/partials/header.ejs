    <nav>
      <ul class="menu">
      <h1 class="logo">Web Dev Journal</h1>
      <li class="item"><a href="/">Home</a></li>
      <li class="item"><a href="/about">About us</a></li>
      <li class="item"><a href="/contact">Contact us</a></li>
      <% if (isAuthenticated){ %>
      <li class="item name">Hi, <%= user.fName || user.name %></li>
      <li class="item button"><a href="/logout">Logout</a></li>
      <% }else{ %>
      <li class="item button"><a href="/signup">Sign Up</a></li>
      <li class="item button secondary"><a href="/login">Log In</a></li>
      <% } %>
      <li class="toggle"><span class="bars"></span></li>
    </ul>
